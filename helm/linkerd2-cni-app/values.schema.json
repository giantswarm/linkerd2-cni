{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "cniPluginImage": {
            "type": "string"
        },
        "cniPluginVersion": {
            "type": "string"
        },
        "destCNIBinDir": {
            "type": "string"
        },
        "destCNINetDir": {
            "type": "string"
        },
        "ignoreInboundPorts": {
            "type": "string"
        },
        "ignoreOutboundPorts": {
            "type": "string"
        },
        "image": {
            "type": "object",
            "properties": {
                "registry": {
                    "type": "string"
                }
            }
        },
        "imagePullSecrets": {
            "type": "array"
        },
        "inboundProxyPort": {
            "type": "integer"
        },
        "installNamespace": {
            "type": "boolean"
        },
        "logLevel": {
            "type": "string"
        },
        "namespace": {
            "type": "string",
            "enum": ["linkerd-cni"]
        },
        "outboundProxyPort": {
            "type": "integer"
        },
        "portsToRedirect": {
            "type": "string"
        },
        "priorityClassName": {
            "type": "string"
        },
        "proxyUID": {
            "type": "integer"
        },
        "resources": {
            "type": "object",
            "properties": {
                "limits": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                },
                "requests": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "useWaitFlag": {
            "type": "boolean"
        }
    }
}
